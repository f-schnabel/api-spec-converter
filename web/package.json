{
  "name": "api-spec-converter-web",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/universal-starter.git"
  },
  "scripts": {
    "postinstall": ".\\node_modules\\.bin\\webpack",
    "ng": "ng",
    "start": "ng serve --port 3000 --host 0.0.0.0",
    "clean": "del .\\dist",
    "build": "npm run clean && npm run build:static",
    "build:gh-pages": "npm run build && rm -rf ../docs && cp -r ./dist/browser ../docs",
    "build:client": "ng build browser",
    "build:server": "ng build --configuration prod --output-hashing=none server",
    "build:static": "npm run build:client && npm run build:server && npm run webpack:server && npm run prerender",
    "build:dynamic": "npm run build:client && npm run build:server && npm run webpack:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress",
    "prerender": "cd dist && node prerender",
    "serve:static": "cd dist/browser && http-server",
    "serve:dynamic": "node dist/server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/platform-server": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@nguniversal/express-engine": "^16.2.0",
    "core-js": "^3.37.0",
    "rxjs": "^7.8.1",
    "webpack": "^5.91.0",
    "zone.js": "^0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.12",
    "@angular/cli": "^16.2.12",
    "@angular/compiler-cli": "^16.2.12",
    "@angular/language-service": "^6.1.10",
    "@types/node": "^8.0.30",
    "apply-loader": "^2.0.0",
    "@babel/core": "^7.24.4",
    "babel-loader": "^9.1.3",
    "babel-polyfill": "^6.26.0",
    "@babel/preset-env": "^7.24.4",
    "bootstrap": "^3.3.7",
    "bootstrap-loader": "^4.0.4",
    "bootstrap-sass": "^3.3.7",
    "cpy-cli": "^1.0.1",
    "file-saver": "^1.3.3",
    "font-awesome": "^4.7.0",
    "http-server": "^0.12.3",
    "jquery": "^3.5.0",
    "markdown-loader": "^5.1.0",
    "pug-html-loader": "^1.1.5",
    "pug-loader": "^2.3.0",
    "pug-plugin-ng": "0.0.2",
    "reflect-metadata": "^0.1.10",
    "ts-loader": "^2.3.7",
    "typescript": "^5.1.6",
    "webpack-cli": "^5.1.4"
  }
}
